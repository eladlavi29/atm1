.global _start

.section .text

  mov head(%rip), %rax
  cmpl $1, (%rax)
  jne bad_exit
  
  mov 4(%rax), %rax
  cmpl $1, (%rax)
  jne bad_exit
  
  mov 4(%rax), %rax
  cmpl $1, (%rax)
  jne bad_exit
  
  mov 4(%rax), %rax
  cmpl $1, (%rax)
  jne bad_exit
  
  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
D: .int 1
.quad 0

C: .int 1
.quad D

B: .int 1
.quad C

A: .int 1
.quad B

head: .quad A

Source: .quad D

Value: .int 6