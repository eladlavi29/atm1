.global _start

.section .text

  mov head(%rip), %rax
  cmpl $4, (%rax)
  jne bad_exit
  
  mov 4(%rax), %rax
  cmpl $6, (%rax)
  jne bad_exit
  
  mov 4(%rax), %rax
  cmpl $-5, (%rax)
  jne bad_exit
  
  mov 4(%rax), %rax
  cmpl $1, 8(%rax)
  jne bad_exit

  
  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
D: .int 1
.quad 0

C: .int -5
.quad D

B: .int 6
.quad C

A: .int 4
.quad B

head: .quad A

Source: .quad C

Value: .int 6